# Copy this file to .env and fill in your values
# NEVER commit .env to git

# Environment variables for local and production
NODE_ENV=development
PORT=5000
MONGO_URI=mongodb://localhost:27017/Unigo
# Use a strong random string for production
JWT_SECRET=your_jwt_secret_here

# For production, use your MongoDB Atlas connection string:
# MONGO_URI=mongodb+srv://<user>:<password>@cluster.mongodb.net/Unigo?retryWrites=true&w=majority

# Your deployed frontend URL (for CORS whitelist in production)
FRONTEND_URL=https://your-app.vercel.app

